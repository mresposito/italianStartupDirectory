# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# pages
GET     /                           org.startupDirectory.pages.Application.index

# GET     /persone                    org.startupDirectory.pages.Application.persone 
# GET     /startup                    org.startupDirectory.pages.Application.startup
# GET     /risorse                    org.startupDirectory.pages.Application.risorse 

GET     /about                      org.startupDirectory.pages.Application.about  
GET     /privacy                    org.startupDirectory.pages.Application.privacy

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
